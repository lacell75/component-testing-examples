{
  "name": "jsdom-shortcomings",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "cypress:run": "cypress run --component",
    "playwright:run": "playwright test --config ./playwright.config.ts",
    "preview": "vite preview",
    "test": "vitest -c ./vitest.conf.ts",
    "test:ui": "vitest -c ./vitest.conf.ts --ui",
    "coverage": "vitest run -c ./vitest.conf.ts --coverage",
    "wdio": "wdio run ./wdio.conf.ts"
  },
  "dependencies": {
    "algoliasearch": "^4.19.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-instantsearch-dom": "^6.40.4"
  },
  "devDependencies": {
    "@playwright/experimental-ct-react": "^1.36.2",
    "@playwright/test": "^1.36.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@types/react-instantsearch-dom": "^6.12.3",
    "@vitejs/plugin-react": "^4.0.4",
    "@wdio/browser-runner": "^8.14.3",
    "@wdio/cli": "^8.14.4",
    "@wdio/mocha-framework": "^8.14.0",
    "@wdio/spec-reporter": "^8.14.0",
    "cypress": "^12.17.3",
    "cypress-real-events": "^1.10.0",
    "jsdom": "^22.1.0",
    "playwright": "^1.36.2",
    "react-zoom-pan-pinch": "^3.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.8",
    "vitest": "^0.34.1"
  }
}
