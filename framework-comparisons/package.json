{
  "name": "jsdom-shortcomings",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "cypress:run": "cypress run --component",
    "playwright:run": "playwright test --config ./playwright.config.ts",
    "preview": "vite preview",
    "test": "vitest -c ./vitest.conf.ts",
    "test:ui": "vitest -c ./vitest.conf.ts --ui",
    "coverage": "vitest run -c ./vitest.conf.ts --coverage",
    "wdio": "wdio run ./wdio.conf.ts"
  },
  "dependencies": {
    "algoliasearch": "^4.15.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-instantsearch-dom": "^6.39.0"
  },
  "devDependencies": {
    "@playwright/experimental-ct-react": "^1.31.2",
    "@playwright/test": "^1.31.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-instantsearch-dom": "^6.12.3",
    "@vitejs/plugin-react": "^3.1.0",
    "@wdio/browser-runner": "^8.5.7",
    "@wdio/cli": "^8.5.7",
    "@wdio/mocha-framework": "^8.5.6",
    "@wdio/spec-reporter": "^8.4.0",
    "chromedriver": "^110.0.0",
    "cypress": "^12.7.0",
    "cypress-real-events": "^1.7.6",
    "jsdom": "^21.1.0",
    "playwright": "^1.31.2",
    "react-zoom-pan-pinch": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vitest": "^0.29.2",
    "wdio-chromedriver-service": "^8.1.1"
  }
}
